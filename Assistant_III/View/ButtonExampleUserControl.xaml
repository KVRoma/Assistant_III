<UserControl x:Class="Assistant_III.View.ButtonExampleUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Assistant_III.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button Grid.Column="2" Content="Save" Margin="220,0,220,7" Foreground="LightGreen" FontSize="15" Command="{Binding SaveExampleCommand}" Height="30" Width="60"></Button>
    </Grid>
</UserControl>
